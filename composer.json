{
  "name": "acquia/acquia-ra-composer",
  "description": "Project json for Acquia Remote Administration Composer builds",
  "type": "project",
  "license": "Proprietary",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "composer/installers": "^1.0",
    "cweagans/composer-patches": "^1.6.0",
    "drupal-composer/drupal-scaffold": "^2.0.0",
    "drupal/admin_toolbar": "^1.0",
    "drupal/core": "8.7.7",
    "drush/drush": "^8.1",
    "swiftmailer/swiftmailer": "5.4.8",
    "beanstream/beanstream": "1.0.1",
    "mikehaertl/php-pdftk": "0.4.0",
    "tecnickcom/tcpdf": "^6.3",
    "algolia/algoliasearch-client-php": "1.25.1",
    "aws/aws-sdk-php": "3.72.0",
    "drupal/content_access": "1.x-dev",
    "drupal/acquia_connector": "^1.16",
    "drupal/ctools": "3.0",
    "drupal/ctools_block": "3.0",
    "drupal/ctools_views": "3.0",
    "drupal/ckeditor_a11ychecker": "1.0",
    "drupal/ckeditor_balloonpanel": "1.1",
    "drupal/ckeditor_media_embed": "1.1",
    "drupal/linkit": "5.0-beta5",
    "drupal/link_css": "1.x-dev",
    "drupal/entityconnect": "2.0-alpha1",
    "drupal/entityqueue": "^1.0",
    "drupal/entity_reference_revisions": "1.3",
    "drupal/viewfield": "3.0-alpha2",
    "drupal/field_group": "1.0-rc6",
    "drupal/url_embed": "1.0-alpha1",
    "drupal/focal_point": "1.0-beta4",
    "drupal/mailsystem": "4.1",
    "drupal/swiftmailer": "1.0-beta1",
    "drupal/colorbox": "1.4",
    "drupal/crop": "1.2",
    "drupal/file_entity": "2.0-beta4",
    "drupal/imce": "1.6",
    "drupal/lang_dropdown": "2.0-beta1",
    "drupal/addtoany": "1.8",
    "drupal/editor_advanced_link": "1.4",
    "drupal/easy_breadcrumb": "1.6",
    "drupal/embed": "1.0",
    "drupal/externalauth": "1.0",
    "drupal/fieldblock": "2.0-alpha1",
    "drupal/fillpdf": "4.4",
    "drupal/fitvids": "1.0",
    "drupal/link_attributes": "1.1",
    "drupal/menu_block": "1.4+1-dev",
    "drupal/pathauto": "1.0",
    "drupal/redirect": "1.0-beta1",
    "drupal/responsive_menu": "2.4",
    "drupal/scheduler": "1.0-rc2",
    "drupal/token": "1.0",
    "drupal/ultimenu": "1.6",
    "drupal/paragraphs": "^1.6",
    "drupal/view_unpublished": "1.0-alpha1",
    "drupal/core_views_facets": "^1.0",
    "drupal/search_api_db": "^1.11",
    "drupal/facets": "^1.3",
    "drupal/facets_summary": "^1.3",
    "drupal/search_api": "^1.11",
    "drupal/search_api_sorts": "1.0-beta1",
    "drupal/search_api_views_taxonomy": "^1.11",
    "drupal/search_autocomplete": "1.1",
    "drupal/search_api_solr": "^1.2",
    "drupal/metatag": "^1.8",
    "drupal/honeypot": "1.27",
    "drupal/google_tag": "1.0",
    "drupal/flood_unblock": "1.4",
    "drupal/better_exposed_filters": "3.0-alpha3",
    "drupal/views_conditional": "1.x-dev",
    "drupal/views_field_view": "1.0-beta2",
    "drupal/menu_normalizer": "1.0",
    "drupal/rest_menu_tree": "1.0",
    "drupal/rest_menu_items": "^2.6",
    "drupal/restui": "1.15",
    "drupal/webform": "^5.2",
    "drupal/webform_node": "^5.2",
    "drupal/webform_ui": "^5.2",
    "sendgrid/sendgrid": "6.0.0",
    "drupal/console": "~1.0",
    "phpoffice/phpspreadsheet": "^1.2",
    "drupal/memcache": "^2.0",
    "dompdf/dompdf": "^0.8.2",
    "drupal/bootstrap": "^3.17",
    "league/oauth2-client": "^2.4",
    "defuse/php-encryption": "^2.2"
  },
  "require-dev": {
    "drupal/devel": "^1.0",
    "drupal/stage_file_proxy": "^1.0"
  },
  "conflict": {
    "drupal/core": "7.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "docroot/core": ["type:drupal-core"],
      "docroot/libraries/{$name}": ["type:drupal-library"],
      "docroot/modules/contrib/{$name}": ["type:drupal-module"],
      "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
      "docroot/themes/contrib/{$name}": ["type:drupal-theme"]
    },
    "enable-patching": true,
    "patches": {
      "drupal/core": {
        "There is no indication on configuration forms if there are overridden values": "https://www.drupal.org/files/issues/there_is_no_indication-2408549-60.patch",
        "drupalimage CKEditor plugin should not require data-entity-uuid and data-entity-type when image upload is disabled": "https://www.drupal.org/files/issues/2018-05-08/drupalimage_ckeditor-2771837-27.patch",
        "Ajax attached to Views exposed filter form does not trigger callbacks": "https://www.drupal.org/files/issues/2842525-17.patch",
        "Page views should also have a more link": "https://www.drupal.org/files/issues/views-page_views_more_link-1427854-64.patch",
        "CORS breaks with cache proxies and same origin usage": "https://www.drupal.org/files/issues/2019-01-16/3001809-23.patch"
      },
      "drupal/menu_block": {
        "Custom patch from OPIN for menu_block": "patches/menu_block/MenuBlock-rcm-follow-and-subtree.patch"
      },
      "drupal/modal_page": {
        "Custom patch from OPIN for modal_page": "patches/modal_page/page_modal_fix.patch"
      },
      "drupal/search_autocomplete": {
        "Remove plural from search autocomplete": "patches/search_autocomplete/search_autocomplete_remove_plural.patch"
      },
      "drupal/swiftmailer": {
        "Add CC and BCC support to Swiftmailer": "https://www.drupal.org/files/issues/swiftmailer_cc_bcc_support-2892104-4.patch"
      },
      "drupal/linkit": {
        "Is there a way to have Linkit link to files directly, rather than to the file entity page?": "patches/linkit/linkit_file_entity_link.patch"
      }
    }
  },
  "scripts": {
  },
  "config": {
    "platform": {
      "php": "7.3"
    }
  }
}
